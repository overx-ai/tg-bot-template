{
  "project_name": "My Telegram Bot",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-').replace('_', '-') }}",
  "project_description": "A Telegram bot built with the telegram-bot-template framework",
  "author_name": "Jack Ma",
  "author_email": "hustlequeen@mail.ru",
  "github_username": "hustlestar",
  "github_repo_name": "{{ cookiecutter.project_slug }}",
  "python_version": "3.11",
  "bot_name": "{{ cookiecutter.project_name }}",
  "bot_username": "{{ cookiecutter.project_slug.replace('-', '_') }}_bot",
  "default_language": "en",
  "supported_languages": "en,ru,es",
  "use_openrouter": "y",
  "openrouter_model": "google/gemini-2.0-flash-001",
  "use_support_bot": "n",
  "database_name": "{{ cookiecutter.project_slug.replace('-', '_') }}_db",
  "use_github_actions": "y",
  "use_docker": "y",
  "open_source_license": "MIT",
  "_copy_without_render": [
    "*.html",
    "*.css",
    "*.js",
    "*.png",
    "*.jpg",
    "*.gif",
    "*.ico",
    "*.svg",
    "*.woff",
    "*.woff2",
    "*.ttf",
    "*.eot"
  ]
}